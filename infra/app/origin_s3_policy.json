{
    "Version":"2012-10-17",
    "Id":"PolicyForCloudFrontPrivateContent",
    "Statement":[
      {
        "Sid": "Grant a CloudFront Origin Identity access to support private content",
        "Effect":"Allow",
        "Principal":{"CanonicalUser":"${account_canonical_user}"},
        "Action":"s3:GetObject",
        "Resource":"${s3_bucket_arn}/*"
      }
    ]
 }